{
  "name": "ppfish",
  "version": "1.1.9",
  "description": "qiyu and prophet ui components",
  "scripts": {
    "precommit": "lint-staged",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:site lint:watch",
    "build": "npm-run-all --parallel build:site build:components",
    "stylelint": "stylelint \"source/**/*.less\"",
    "lint": "esw webpack.config.* source tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build:components": "babel-node tools/buildComponents.js",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "test:watch": "npm run test -- --watchAll",
    "test:debug": "node --inspect ./node_modules/jest-cli/bin/jest.js --runInBand",
    "analyze": "babel-node ./tools/analyzeBundle.js",
    "open:site": "babel-node tools/openSite.js",
    "build:site": "babel-node tools/buildSite.js",
    "prepublish:site": "tools/prepare.sh",
    "publish:site": "tools/release.sh"
  },
  "main": "source/components/index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "date-fns": "^1.28.5",
    "debug": "^3.1.0",
    "lodash.debounce": "^4.0.8",
    "query-string": "^4.2.2",
    "rc-trigger": "^2.5.4",
    "react-draggable": "^3.0.5",
    "react-quill": "^1.3.0",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "antd": ">= 3.5.3",
    "echarts": "^3.7.2",
    "prop-types": "> 15.5.10",
    "react": ">= 16.1.1",
    "react-dom": ">= 16.1.1"
  },
  "devDependencies": {
    "antd": "^3.5.3",
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "babel-standalone": "^6.26.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "codemirror": "^5.38.0",
    "copy-to-clipboard": "^3.0.8",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.5.7",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.2",
    "echarts": "^3.7.2",
    "ejs-loader": "^0.3.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "3.4.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.1",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "isparta": "^4.0.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "json-server": "^0.12.0",
    "jsonp": "^0.2.1",
    "jsop": "^0.3.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.0",
    "lint-staged": "^5.0.0",
    "loader-utils": "^1.1.0",
    "marked": "^0.4.0",
    "moment": "^2.22.2",
    "npm-run-all": "3.0.0",
    "opn": "^5.3.0",
    "opn-cli": "^3.1.0",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.5",
    "pre-commit": "^1.2.2",
    "prismjs": "^1.14.0",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^4.3.2",
    "react-loadable": "^5.4.0",
    "react-markdown": "^3.3.0",
    "react-router": "^3.2.1",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "^2.4.0",
    "rimraf": "2.5.4",
    "style-loader": "^0.18.1",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^0.5.8",
    "uuid": "^3.1.0",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.19.1"
  },
  "lint-staged": {
    "linters": {
      "source/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "tools/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "source/**/*.less": [
        "stylelint --fix",
        "git add"
      ]
    },
    "globOptions": {
      "matchBase": true,
      "dot": true
    }
  },
  "author": "and80506 <and80506@163.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NSFI/ppfish-components.git"
  },
  "keywords": [
