  "lint-staged": {
    "linters": {
      "source/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "tools/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "source/**/*.less": [
        "stylelint --fix",
        "git add"
      ]
    },
    "globOptions": {
      "matchBase": true,
      "dot": true
    }
