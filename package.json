{
  "name": "ppfish-components",
  "version": "1.0.0",
  "description": "qiyu and prophet ui components",
  "scripts": {
    "precommit": "npm run lint",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node tools/mock.js",
    "open:src": "babel-node --max_old_space_size=2048 tools/srcServer.js",
    "start:debug": "babel-node $NODE_DEBUG_OPTION tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "stylelint": "stylelint \"source/**/*.less\"",
    "lint": "esw webpack.config.* source tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "test": "jest --setupTestFrameworkScriptFile=raf/polyfill",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "test:watch": "npm run test -- --watch",
    "analyze": "babel-node ./tools/analyzeBundle.js"
  },
  "dependencies": {
    "antd": "^2.13.4",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.6",
    "date-fns": "^1.28.5",
    "debug": "^3.1.0",
    "echarts": "^3.7.2",
    "js-cookie": "^2.1.2",
    "lodash": "^4.16.6",
    "lodash.debounce": "^4.0.8",
    "papaparse": "^4.3.6",
    "particles.js": "^2.0.0",
    "prop-types": "^15.5.10",
    "query-string": "^4.2.2",
    "rc-queue-anim": "^1.3.1",
    "react": "^16.1.1",
    "react-amap": "^1.0.3",
    "react-dom": "^16.1.1",
    "react-dom-factories": "^1.0.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.0-rc.1",
    "redux": "^3.5.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.13.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "browser-sync": "^2.18.13",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.2",
    "ejs-loader": "^0.3.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "3.4.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.1",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "isparta": "^4.0.0",
    "jest": "^21.0.2",
    "jest-cli": "^21.0.2",
    "json-server": "^0.12.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "less-vars-to-js": "^1.2.0",
    "lint-staged": "^5.0.0",
    "loader-utils": "^1.1.0",
    "npm-run-all": "3.0.0",
    "ora": "^1.3.0",
    "postcss-loader": "^2.0.5",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "redux-logger": "^2.4.0",
    "rimraf": "2.5.4",
    "style-loader": "^0.18.1",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "url-loader": "^0.5.8",
    "uuid": "^3.1.0",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "lint-staged": {
    "linters": {
      "source/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "tools/**/*.js": [
        "eslint --fix",
        "git add"
      ],
      "source/**/*.less": [
        "stylelint --fix",
        "git add"
      ]
    },
    "globOptions": {
      "matchBase": true,
      "dot": true
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
    }
  },
  "author": "wushengzhu <wushengzhu@corp.easenet.com>",
  "license": "MIT"
}
